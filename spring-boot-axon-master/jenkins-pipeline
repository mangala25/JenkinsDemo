stage('dev'){
	echo 'dev build runs'
}
stage('stag'){
	node {
		def exists = fileExists 'spring-boot-axon-master/pom.xml'
		//echo exists
		if(fileExists(exists)){
			echo 'File found'
		} else{
			error 'File not found'
		}
	}
}
stage('pre-prod'){
	echo 'pe-prod build runs'
}
stage('prod'){
	echo 'prod build runs'
}